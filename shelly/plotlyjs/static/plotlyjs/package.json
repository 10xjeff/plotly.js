{
  "name": "plotlyjs",
  "version": "1.0.0",
  "description": "plotly.js library",
  "repository": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dev": "npm run browserify-dev; npm run sass-dev",
    "build-prod": "npm run browserify-prod; npm run sass-prod",
    "browserify-dev": "browserify index.js --debug --standalone Plotly -o build/plotlyjs-bundle.js",
    "browserify-prod": "browserify index.js --standalone Plotly | uglifyjs > build/plotlyjs-bundle.js",
    "sass-dev": "node-sass --output-style expanded src/css/style.scss build/plotly-style.css",
    "sass-prod": "node-sass --output-style compressed src/css/style.scss build/plotly-style.css"
  },
  "author": "Plotly amazing team",
  "license": "ISC",
  "dependencies": {
    "browserify": "^8.1.1",
    "node-sass": "^2.0.0-beta",
    "uglify-js": "^2.4.16"
  }
}
